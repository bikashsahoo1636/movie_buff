<html>

<h3>LIST OF YOUR MOVIES:</h3>

{{#each error}}
    <div class="error-background"> <div class="error"> {{msg}} </div> </div>
{{/each}}


<div class= "rating-container">
    <h3>My Rating Entries: </h3>
    <table class= "ratings-table">
        <tr>
            <th>Movie You Have Watched: </th>
            <th>Movies You Want to Watch: </th>
            {{!-- <th>Options</th> --}}
        </tr>
        <tr class="ratings-table-body">
            {{#each rating_entry}}
            <tr>
                <td>{{movie_title}}</td>
                <td>{{rating_given}}</td>
                {{!-- <td><button onclick="delete_rating({{ratingID}})">Delete</button><a class="update-button"href="/rating/{{ratingID}}">Update</a></td> --}}
                {{!-- <td></td> --}}
            </tr>
            {{/each}}
        </tr>
    </table>

</div>
<br/>

    


</html>
